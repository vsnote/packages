{
  "name": "om-nestjs-common",
  "version": "0.0.5",
  "description": "Nest common",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b -v",
    "clean": "tsc -b --clean",
    "format": "prettier \"**/*.ts\" \"**/*.json\" --ignore-path ./.prettierignore --write && git status",
    "lint": "tslint -p tsconfig.json",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A . && git commit -m \"chore: format\"",
    "postversion": "git push && git push --tags",
    "publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vsnote/packages.git"
  },
  "author": "votan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vsnote/packages/issues"
  },
  "homepage": "https://github.com/vsnote/packages#readme",
  "dependencies": {
    "@fleekhq/fleek-storage-js": "^1.0.24",
    "@nestjs/common": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "ethers": "^5.6.9",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ]
}
